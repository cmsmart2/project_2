<div id="layout">
  <!-- Menu toggle -->
  <a href="#menu" id="menuLink" class="menu-link">
    <!-- Hamburger icon -->
    <span></span>
  </a>

  <div id="menu">
    <div class="pure-menu">
      <a class="pure-menu-heading" href="#">Drink Menu</a>

      <ul class="pure-menu-list">
        <li class="pure-menu-item"><a href="/" class="pure-menu-link">Home</a></li>
        <li class="pure-menu-item"><a href="/favorites" class="pure-menu-link">Favorites</a></li>
        <li class="pure-menu-item"><a href="#" class="pure-menu-link">Pantry</a></li>

        
    </ul>
    </div>
  </div>

  <div id="main">
    
    <div class="header">
      <h1>Cocktail Party</h1>
    </div>

    <div class="row">
      <div class="col-8 offset-2">
        <form class="pure-container mb-4" action="POST">
          <form class="pure-form"> 
       <input type="text" class="pure-input-squared" placeholder="Margarita"> 
       <button type="submit" class="pure-button">Search</button> 
     </form>
          </div>
          <div class="form-group">
          </div>
        <div class="swiper-container">
          <div class="swiper-wrapper">
            <div class="swiper-slide" style="background-image:url(https://via.placeholder.com/150)"></div>
            <div class="swiper-slide" style="background-image:url(https://via.placeholder.com/150)"></div>
            <div class="swiper-slide" style="background-image:url(https://via.placeholder.com/150)"></div>
          </div>
          <div class="swiper-pagination"></div>
        </div>
        </form>
        <h3>Footer goes here</h3>
        <ul id="example-list" class="list-group">
          {{#each examples}}
          <li data-id="{{this.id}}" class="list-group-item">
            <a href="example/{{this.id}}">{{this.text}}</a>
            <button class="btn btn-danger float-right delete">ï½˜</button>
          </li>
          {{/each}}
        </ul>
      </div>
    </div>
  </div>
</div>
